import java.util.Scanner;


public class Solution {
    public static void main(String[] args)  {
        Scanner Sc=new Scanner(System.in);
        int n=Sc.nextInt();
        for(int i=1;i<=10;i++){
            int result=n*i;
            System.out.println(n+" x "+i+" = "+result);
            
        }
    }
}
