public class Solution {  

    public static void pushZerosAtEnd(int[] arr) {
        //Your code goes here

        int nonZero=0;

        for( int i=0;i<arr.length;i++){
            if(arr[i]!=0){
                arr[nonZero]=arr[i];
                nonZero++;
            }
            
        }
        for(int i=nonZero;i<arr.length;i++){
            arr[i]=0;
        }
    }

}
