
import java.util.Scanner;


// Remember that the class name should be "Main" and should be "public".

    public class Main{
	public static void main(String[] args) {
	
	Scanner Sc=new Scanner(System.in);
	int total_testcase=Sc.nextInt();
	for(int i=1;i<=total_testcase;i++){
	    int initial_budget=Sc.nextInt();
	int spend_money=Sc.nextInt();
	int first_sports=Sc.nextInt();
	int second_sports=Sc.nextInt();
	int third_sports=Sc.nextInt();
	int remaining_money=(initial_budget-spend_money);
	int wanted_amount=(first_sports+second_sports+third_sports);
	
	if(remaining_money>=wanted_amount){
	    System.out.println("YES");
	}else{
	    System.out.println("NO");
	}
	    
	}
	
	}
	
}
