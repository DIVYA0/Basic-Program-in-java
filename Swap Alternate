
public class Solution {

    public static void swapAlternate(int arr[]) {
        //Your code goes here
        int temp=0;
        if{
               for(int i=0;i<arr.length-1;i+=2){
                temp=arr[i];
                arr[i]=arr[i+1];
                arr[i+1]=temp;
            } 
            }
}
